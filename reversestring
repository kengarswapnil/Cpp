#include<iostream>
#include<string>
using namespace std;
 
 void str1(const string& str,int idx)
 {
   if(idx==0)
   {
     cout <<str[idx];
     return;
     }
     
     cout <<str[idx];
     str1(str,idx-1);
}
  int main(){
  
  string str ="swapnil";
  str1(str,str.length()-1);
      
    return 0;
}